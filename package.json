{
  "name": "modelizr",
  "version": "0.0.1",
  "description": "A light-weight js library for managing data store",
  "main": "dist/index.js",
  "scripts": {
    "build:commonjs": "better-npm-run build:commonjs",
    "build:umd": "better-npm-run build:umd",
    "build:umdmin": "uglifyjs dist/modelizr.js -o dist/modelizr.min.js",
    "build:es": "babel -d es/ src/",
    "build": "npm run reset && npm run build:commonjs && npm run build:umd && npm run build:umdmin && npm run build:es",
    "reset": "better-npm-run reset:folder",
    "test": "test"
  },
  "betterScripts": {
    "build:commonjs": {
      "command": "babel -d lib/ src/",
      "env": {
        "NODE_ENV": "commonjs"
      }
    },
    "build:umd": {
      "command": "mkdirp dist/ && babel -o dist/modelizr.js src/",
      "env": {
        "NODE_ENV": "umd"
      }
    },
    "reset:folder": {
      "command": "rm -rf lib dist es"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daiyanze/Modelizr.git"
  },
  "keywords": [
    "modelizr",
    "normalizr",
    "database",
    "redux",
    "react"
  ],
  "author": "daiyanze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daiyanze/Modelizr/issues"
  },
  "homepage": "https://github.com/daiyanze/Modelizr#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-1": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "eslint": "^4.16.0",
    "mkdirp": "^0.5.1",
    "uglify-js": "^3.3.9"
  }
}
