{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "cammelcase": 0,
    "comma-dangle": [2],
    "comma-spacing": [2, { "before": false, "after": true }],
    "indent": [2, 2, { "SwitchCase": 1 }],
    "object-curly-spacing": [2, "always"],
    "prefer-const": 2,
    "quotes": [1, "single", { "avoidEscape": true }],
    "prefer-arrow-callback": 2
  }
}